<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<parameter name="browser" value="Chrome"></parameter>
<listeners>
  <listener class-name="listeners.Listener"></listener>
  </listeners>
  <test thread-count="5" name="Test">
    <classes>
      <class name="testscript.LoginTest"><methods>
      <include name="verifyUserLoginWithValidCredentials"></include>
      <include name="verifyUserLoginWithInvalidUsernameAndValidPassword"></include>
      </methods></class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
